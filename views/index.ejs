<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="UTF-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<link rel="stylesheet" href="/styles.css">
	<title>Inventory app</title>
</head>
<body>
	<h1>Manufacturers</h1>
	<button>
		<a href="/add-manufacturer">
			Add a manufacturer
		</a>
	</button>
	<% manufacturers.forEach(manufacturer => { %>
		<div>
			<a href="/<%= manufacturer.name %>">
				<h2><%= manufacturer.name %></h2>
				<div>Founded: <%= manufacturer.founded %></div>
				<div>Owner: <%= manufacturer.owner %></div>
				<button>
					<a href="/<%= manufacturer.name %>/update">
						Update
					</a>
				</button>
				<button>
					<a href="/<%= manufacturer.name %>/del">
						Delete
					</a>
				</button>
			</a>
		</div>
	<% }) %>
</body>
</html>